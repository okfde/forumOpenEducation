{% assign attendees = include.attendees | split: ',' %}

<div class="content-element infobox {{ include.float }}">
    <p class="infobox-headline">{{ include.headline }}</p>
    <ul class="infobox-copy">
        {% for attendees in attendees %}
            <li class="infobox-copy">{{ attendees }}</li>
        {% endfor %}
    </ul>
    <p class="infobox-headline">Moderation</p>
    <span class="infobox-copy">{{ include.moderation }}</span>
    {% if include.print %}
        <center><a class='edu-btn white feedback' href='{{ include.print }}'>Ergebnisse Drucken</a></center>
    {% endif %}
</div>
